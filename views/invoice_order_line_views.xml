<odoo>
    <record id="invoice_order_line_product_brand_id" model="ir.ui.view">
        <field name="name">invoice.order.line.product.brand.id</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='invoice_tab']/field[@name='invoice_line_ids']/tree/field[@name='product_id']" position="after">
                <field name="product_brand"/>
            </xpath>
        </field>
    </record>
</odoo>